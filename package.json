{
  "name": "2025-advent-of-code",
  "version": "1.0.0",
  "description": "2025 Advent of Code solutions in TypeScript",
  "homepage": "https://github.com/dustinkesner/2025-advent-of-code#readme",
  "bugs": {
    "url": "https://github.com/dustinkesner/2025-advent-of-code/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinkesner/2025-advent-of-code.git"
  },
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watchAll --watchman=false",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start:day1": "npm run build && node dist/day1/index.js",
    "test:day1": "jest day1/index.test.ts",
    "start:day2": "npm run build && node dist/day2/index.js",
    "test:day2": "jest day2/index.test.ts",
    "start:day3": "npm run build && node dist/day3/index.js",
    "test:day3": "jest day3/index.test.ts",
    "start:day4": "cp day4/input.txt dist/day4/ 2>/dev/null || true && npm run build && node dist/day4/index.js",
    "test:day4": "jest day4/index.test.ts",
    "start:day5": "cp day5/input.txt dist/day5/ 2>/dev/null || true && npm run build && node dist/day5/index.js",
    "test:day5": "jest day5/index.test.ts",
    "start:day6": "cp day6/input.txt dist/day6/ 2>/dev/null || true && npm run build && node dist/day6/index.js",
    "test:day6": "jest day6/index.test.ts",
    "start:day7": "cp day7/input.txt dist/day7/ 2>/dev/null || true && npm run build && node dist/day7/index.js",
    "test:day7": "jest day7/index.test.ts",
    "start:day8": "npm run build && node dist/day8/index.js",
    "test:day8": "jest day8/index.test.ts",
    "start:day9": "cp day9/input.txt dist/day9/ 2>/dev/null || true && npm run build && node dist/day9/index.js",
    "test:day9": "jest day9/index.test.ts",
    "start:day10": "npm run build && node dist/day10/index.js",
    "test:day10": "jest day10/index.test.ts",
    "start:day11": "npm run build && node dist/day11/index.js",
    "test:day11": "jest day11/index.test.ts",
    "start:day12": "npm run build && node dist/day12/index.js",
    "test:day12": "jest day12/index.test.ts"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}
